{
    "name": "bnomei/kirby3-lapse",
    "type": "plugin",
    "version": "1.0.4",
    "description": "Cache any data until set expiration time",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Meilick",
            "email": "b@bnomei.com"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "cache",
        "expire",
        "simple",
        "performance",
        "key-value"
    ],
    "suggest": {
    },
    "autoload": {
        "files": [
            "config.php"
        ],
        "psr-4": {
            "Bnomei\\": "classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.0",
        "getkirby/cms": "^3.0.0-RC-2.0"
    },
    "conflict": {
        "getkirby/kirby": "<3.0"
    },
    "scripts": {
        "zip": [
            "touch kirby3-lapse.zip",
            "rm kirby3-lapse.zip",
            "composer install --no-dev",
            "composer remove getkirby/cms",
            "composer dumpautoload -o",
            "zip -r kirby3-lapse.zip . -x *.git*",
            "composer require getkirby/cms:'^3.0.0-RC-2.0'"
        ]
    }
}
